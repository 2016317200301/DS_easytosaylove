#include<stdio.h>
main()
{
    int M[9]={1,2,3,4,5,6,7,8,9};    /* 提供一个约瑟夫环 */
    int m=4;                         /* 规定报数为4的出列 */
    int key,s=1;                     /* key用于记录出列的数值，s为出列的位置 */
    int i=9,j;                       /* 循环变量 */
    while(i!=0)
    {
        s=(s+m-1)%i;
        if(s==0)                      /* 公式中对i的余数为0，则s=i */
            s=i;
        key=M[s-1];                   /* 记录出列数值，注意数组从0下标开始 */
        for(j=s-1;j<i;j++)
        {
            M[j]=M[j+1];              /* 把出列的数值后面的数值依次补上 */
        }
        M[i-1]=key;                   /* 数组最后一个位置放出列的数值 */
        i--;                          /* 该约瑟夫环少了一个数值 */
        printf("%d ",key);
    }
}
