    p=L->next;
    q=p->next;
    r=q->next;
    while (q!=L)
    {
        while((p!=L)&&(p->data>q->data))
                p=p->back;
                q->back->next=r;
                r->back=q->back;//(1)
                q->next=p->next;
                q->back=p;
                p->next=q;//(2)
                r->back-back=q;//(3)
                q=r;
                p=q->back;
                r=L->next;//(4)
}
}
