typedef struct{

    int elem[MAXSIZE];

    int length;

}SeqList;



int odd(int x){

    if(x%2!=0){

        return 1;

    }else{

        return 0;

    }

}





void exchange(SeqList *L){

    int a,b,temp;

    a=0;

    b=L->length-1;

    while(a<b){

        while(odd(L->elem[a])){

            a++;

        }

        while(!odd(L->elem[b])){

            b--;

        }

        if(a<b){

            temp = L->elem[a];

            L->elem[a] = L->elem[b];

            L->elem[b] = temp;

        }

    }

}



int main(){

    SeqList *L;

    int i;

    L = (SeqList*)malloc(sizeof(SeqList));

    L->elem[0]=1;

    L->elem[1]=4;

    L->elem[2]=3;

    L->elem[3]=4;

    L->length = 4;

    exchange(L);

    for(i=0;i<4;i++){

        printf("%d",L->elem[i]);

    }

    return 0;

}
